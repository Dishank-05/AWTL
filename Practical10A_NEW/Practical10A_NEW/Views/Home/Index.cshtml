@model Practical10A_NEW.Models.Student
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        @using (Html.BeginForm("Myform"))
        {
            @Html.TextBoxFor(x => x.Id) <br /> <br /> <br />
            @Html.TextBoxFor(x => x.Name) <br /> <br /> <br />
            @Html.TextBoxFor(x => x.Email) <br /> <br /> <br />
            <input type="submit" value="submit" />
        }
    </div>
</body>
</html>
